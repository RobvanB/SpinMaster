<p id="notice"><%= notice %></p>

<h1>Listing File Stores</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>Date loaded</th>
      <th>Date exercised</th>
      <th>Attachment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_stores.each do |file_store| %>
      <tr>
        <td><%= file_store.description %></td>
        <td><%= file_store.date_loaded %></td>
        <td><%= file_store.date_exercised %></td>
        <td><%= file_store.attachment %></td>
        <td><%= link_to 'Show', file_store %></td>
        <td><%= link_to 'Edit', edit_file_store_path(file_store) %></td>
        <td><%= link_to 'Destroy', file_store, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File store', new_file_store_path %>
